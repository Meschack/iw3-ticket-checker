{
  "expo": {
    "name": "IW3TicketChecker",
    "slug": "IW3TicketChecker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "iw3ticketchecker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { "supportsTablet": true, "bundleIdentifier": "com.godwingbewezoun.IW3TicketChecker" },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.godwingbewezoun.IW3TicketChecker"
    },
    "web": { "bundler": "metro", "output": "static", "favicon": "./assets/images/favicon.png" },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "fade": true,
          "duration": 1000
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": { "typedRoutes": true },
    "extra": { "router": {}, "eas": { "projectId": "0ab08078-508c-4bbd-a3c7-97a4db7cbea2" } },
    "owner": "godwingbewezoun"
  }
}
